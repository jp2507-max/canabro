{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "bundleIdentifier": "com.canabro.app",
        "buildConfiguration": "Release",
        "scheme": "canabro"
      },
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production-ios": {
      "extends": "production",
      "platform": "ios",
      "ios": {
        "bundleIdentifier": "com.canabro.app",
        "buildConfiguration": "Release",
        "scheme": "canabro",
        "simulator": false,
        "enterpriseProvisioning": "universal"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
