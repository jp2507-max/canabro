{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release",
        "scheme": "canabro"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_UNSTABLE_METRO_OPTIMIZE_GRAPH": "1",
        "EXPO_UNSTABLE_TREE_SHAKING": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
